# Development environment configuration

# Database configuration - uses Docker Compose PostgreSQL
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/automart

# Session secret (for development only)
SESSION_SECRET=dev_session_secret_change_in_production

# Node environment
NODE_ENV=development

# Host configuration 
HOST=0.0.0.0
PORT=5000

# Set to true to seed the database on first run
SEED_DATABASE=true

# Stripe API keys (add your test keys here)
# STRIPE_SECRET_KEY=sk_test_your_key_here
# VITE_STRIPE_PUBLIC_KEY=pk_test_your_key_here